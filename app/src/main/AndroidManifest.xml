<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xiaoyu.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="xiaoyu.sample.IndexTPActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="xiaoyu.sample.BasicTPActivity" android:screenOrientation="landscape" />
        <activity android:name="xiaoyu.sample.MultiTypeTPActivity" />
        <activity android:name="xiaoyu.sample.MultiTypeFixedTPActivity" />
        <activity android:name="xiaoyu.sample.SwipeActivity" />
        <activity android:name="xiaoyu.sample.HorizonPageActivity" />
        <activity android:name="xiaoyu.sample.ViewPagerTPActivity" />

    </application>

</manifest>